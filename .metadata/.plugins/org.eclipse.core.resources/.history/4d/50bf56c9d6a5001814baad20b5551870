package testProject;

public class CommonFuntion {
	private Integer age;
	private String name;
	
	public CommonFuntion(Integer age, String name) {
		this.age = age;
	}
	
	/**
	 * 和获取年龄
	 * @param age
	 * @return
	 */
	public Integer getAge(Integer age){
		return age;
	}
	
	/**
	 * 获取姓名
	 * @param name
	 * @return
	 */
	public String getName(String name){
		return name;
	}
}
